
<span>Periode: @(Tijdlijn.Periode.ToString())</span>
<table class="table table-hover">
    <tbody>
        @foreach(var waarde in Tijdlijn.Waarden)
        {
            <tr>
                <td>@(waarde.Key)</td>
                <td>@(waarde.Value)</td>
            </tr>
        }
    </tbody>
</table>